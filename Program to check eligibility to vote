#include<iostream>
using namespace std;
int main()
{
	int age;
	cout<<"Enter your age to check eligibility:"; cin>>age;
	if (age<18)
	{
		cout<<"You are not eligible to vote";
	}
	else if (age>18)
	{
		cout<<"You are eligible to vote";
	}
	else
	{
		cout<<"Congratulations! You are eligible to vote now";
	}
	return 0;
}
